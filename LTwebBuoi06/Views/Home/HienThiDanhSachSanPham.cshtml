@model LTwebBuoi06.ViewModels.SannPham_Loai_ViewModel
@{
    ViewBag.Title = "HienThiDanhSachSanPham";
}

<h2>HienThiDanhSachSanPham</h2>

<div class="container">
    <h2 class="section-title">Sản Phẩm Nổi Bật</h2>
    <ul>
        @foreach( var item in Model.loais)
            {
                <li>@item.TenLoai</li>
            }
    </ul>
    <div class="card-container">
        @foreach (var item in Model.sanphams)
        {
            <!-- Card 1 -->
            <div class="card">
                <div class="card-img-container">
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRpOlBz1o7vg5nzyUQbNjAR0UXQVqx0-9CXKg&s" alt="Sản phẩm 1" class="card-img">

                </div>
                <div class="card-content">
                    <h3 class="card-title">@item.TenSP</h3>
                    <p class="card-text">Gia: @item.Gia</p>
                    <a href="detail.html" class="card-button">Xem Chi Tiết</a>
                </div>
            </div>
            <!-- Card 2 -->
            <div class="card">
                <div class="card-img-container">
                    <img src="data:image/jpeg;base64," alt="Sản phẩm 2" class="card-img">

                </div>
                <div class="card-content">
                    <h3 class="card-title">@item.TenSP</h3>
                    <p class="card-text">Gia: @item.Gia</p>
                    <a href="detail.html" class="card-button">Xem Chi Tiết</a>
                </div>
            </div>
         
          
        }
    </div>
</div>     